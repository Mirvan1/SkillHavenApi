{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source={SkillHavenSQLMachine};Initial Catalog={SkillHavenDBName};Integrated Security=True;Encrypt=false;"

  },
  "MailSettings": {
    "Server": "smtp.gmail.com",
    "Port": 587,
    "SenderName": "SkillHaven",
    "SenderEmail": "mirvan.sadiqli@gmail.com",
    "UserName": "mirvan.sadiqli@gmail.com",
    "Password": "{SkillHavenMailPassword}"
  },


  "ImagesFolderPath": "Resources/images",
  "ImageExtension": "png",
  "secret-key": "{secret-key-value}",
  "clienturl-forgot-password": "http://localhost:4200/reset-password",
  "SkilRater": {
    "NormBlog": 10,
    "NormRead": 20,
    "NormVote": 15
  },
  "CorsPolicy": {
    "AllowedOrigins": "http://localhost:8000,http://localhost:4200,http://127.0.0.1:8080"
  },
  "ShowSwagger": "true",
  "captcha-ui-key": "{captcha-ui-key-value}"


